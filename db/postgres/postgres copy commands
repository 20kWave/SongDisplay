COPY songs (song_name, upload_time, tag, song_art_url, song_data_url, background_light, background_dark, waveform_data, song_duration, artist_id) from '/Users/Britt-Britt1/hackReactor/SongDisplay/db/songs.csv' delimiter '|' CSV HEADER;

